<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="290.0" prefWidth="345.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.GraphicCardController">
    <children>
        <Label layoutX="20.0" layoutY="20.0" text="Name" />
        <TextField fx:id="inName" layoutX="176.0" layoutY="16.0" />
        <Label layoutX="20.0" layoutY="50.0" text="Manufacturer" />
        <TextField fx:id="inManufac" layoutX="176.0" layoutY="46.0" />
      <Label layoutX="18.0" layoutY="83.0" text="Watts Required" />
      <TextField fx:id="inWatts" layoutX="176.0" layoutY="78.0" />
        <Label layoutX="20.0" layoutY="117.0" text="Price" />
        <TextField fx:id="inPrice" layoutX="176.0" layoutY="113.0" />
        <Label layoutX="20.0" layoutY="147.0" text="RAM (GB)" />
        <TextField fx:id="inRAM" layoutX="176.0" layoutY="143.0" />
        <Label layoutX="20.0" layoutY="177.0" text="RAM Type" />
        <TextField fx:id="inType" layoutX="176.0" layoutY="173.0" />
        <Label layoutX="20.0" layoutY="207.0" text="Frequenzy (MHz)" />
        <TextField fx:id="inHertz" layoutX="176.0" layoutY="203.0" />
        <Button fx:id="regButton" layoutX="77.0" layoutY="242.0" mnemonicParsing="false" onAction="#registerGraphicCard" text="Register" />
        <Button fx:id="cancelButton" layoutX="216.0" layoutY="242.0" mnemonicParsing="false" onAction="#cancelRegistration" text="Cancel" />
    </children>
</AnchorPane>
