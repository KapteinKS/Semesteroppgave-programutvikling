<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="250.0" prefWidth="345.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.FanController">
   <children>
      <Label layoutX="20.0" layoutY="20.0" text="Name" />
      <TextField fx:id="inName" layoutX="176.0" layoutY="16.0" />
      <Label layoutX="20.0" layoutY="50.0" text="Manufacturer" />
      <TextField fx:id="inManufacturer" layoutX="176.0" layoutY="46.0" />
      <Label layoutX="20.0" layoutY="80.0" text="Price" />
      <TextField fx:id="inPrice" layoutX="176.0" layoutY="76.0" />
      <Label layoutX="20.0" layoutY="110.0" text="Fan Diameter" />
      <TextField fx:id="inDiameter" layoutX="176.0" layoutY="106.0" />
      <Label layoutX="20.0" layoutY="140.0" text="Air Pressure" />
      <TextField fx:id="inPressure" layoutX="176.0" layoutY="136.0" />
      <Label layoutX="20.0" layoutY="170.0" text="Max Noise Volume" />
      <TextField fx:id="inNoiseVolume" layoutX="176.0" layoutY="166.0" />
      <Button fx:id="regButton" layoutX="59.0" layoutY="205.0" mnemonicParsing="false" onAction="#registerFan" text="Register" />
      <Button fx:id="cancelButton" layoutX="235.0" layoutY="205.0" mnemonicParsing="false" onAction="#cancelRegistration" text="Cancel" />
   </children>
</AnchorPane>
