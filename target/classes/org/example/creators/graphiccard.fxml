<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="250.0" prefWidth="345.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.controllers.GraphicCardController">
    <children>
        <Label layoutX="20.0" layoutY="20.0" text="Name" />
        <TextField fx:id="inName" layoutX="176.0" layoutY="16.0" />
        <Label layoutX="20.0" layoutY="50.0" text="Manufacturer" />
        <TextField fx:id="inManufac" layoutX="176.0" layoutY="46.0" />
        <Label layoutX="20.0" layoutY="80.0" text="Price" />
        <TextField fx:id="inPrice" layoutX="176.0" layoutY="76.0" />
        <Label layoutX="20.0" layoutY="110.0" text="RAM (GB)" />
        <TextField fx:id="inRAM" layoutX="176.0" layoutY="106.0" />
        <Label layoutX="20.0" layoutY="140.0" text="RAM Type" />
        <TextField fx:id="inType" layoutX="176.0" layoutY="136.0" />
        <Label layoutX="20.0" layoutY="170.0" text="Frequenzy (MHz)" />
        <TextField fx:id="inHertz" layoutX="176.0" layoutY="166.0" />
        <Button fx:id="regButton" layoutX="77.0" layoutY="205.0" mnemonicParsing="false" onAction="#registerGraphicCard" text="Register" />
        <Button fx:id="cancelButten" layoutX="216.0" layoutY="205.0" mnemonicParsing="false" onAction="#cancelRegistration" text="Cancel" />
    </children>
</AnchorPane>
