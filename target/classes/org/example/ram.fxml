<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="278.0" prefWidth="345.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.RamController">
    <children>
        <Label layoutX="24.0" layoutY="19.0" text="Name" />
        <TextField fx:id="inName" layoutX="159.0" layoutY="14.0" />
        <Label layoutX="25.0" layoutY="48.0" text="Manufacturer" />
        <TextField fx:id="inManufac" layoutX="158.0" layoutY="44.0" />
        <Label layoutX="26.0" layoutY="113.0" text="Size (GB)" />
        <TextField fx:id="inSize" layoutX="159.0" layoutY="109.0" />
        <Label layoutX="26.0" layoutY="143.0" text="Memory Type" />
        <TextField fx:id="inType" layoutX="159.0" layoutY="139.0" />
        <Label layoutX="26.0" layoutY="173.0" text="How many sticks" />
        <TextField fx:id="inSticks" layoutX="159.0" layoutY="169.0" />
        <Button fx:id="regButton" layoutX="83.0" layoutY="210.0" mnemonicParsing="false" onAction="#registerRAM" text="Register" />
        <Button fx:id="cancelButton" layoutX="222.0" layoutY="210.0" mnemonicParsing="false" onAction="#cancelRegistration" text="Cancel" />
        <Label layoutX="26.0" layoutY="81.0" text="Price" />
        <TextField fx:id="inPrice" layoutX="159.0" layoutY="77.0" />
    </children>
</AnchorPane>
