<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="359.0" prefWidth="345.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.CpuController">
    <children>

        <Label layoutX="20.0" layoutY="18.0" text="Name" />
        <TextField fx:id="inName" layoutX="154.0" layoutY="14.0" />
        <Label layoutX="20.0" layoutY="48.0" text="Manufacturer" />
        <TextField fx:id="inManufac" layoutX="153.0" layoutY="44.0" />
        <Label layoutX="20.0" layoutY="78.0" text="Price" />
        <TextField fx:id="inPrice" layoutX="153.0" layoutY="74.0" />
        <Label layoutX="20.0" layoutY="108.0" text="Threads" />
        <TextField fx:id="inThreads" layoutX="153.0" layoutY="104.0" />
        <Label layoutX="20.0" layoutY="138.0" text="Clock Speed (GHz)" />
        <TextField fx:id="inClockSpeed" layoutX="153.0" layoutY="134.0" />
        <Button fx:id="regButton" layoutX="77.0" layoutY="175.0" mnemonicParsing="false" onAction="#registerCPU" text="Register" />
        <Button fx:id="cancelButton" layoutX="216.0" layoutY="175.0" mnemonicParsing="false" onAction="#cancelRegistration" text="Cancel" />

    </children>
</AnchorPane>
